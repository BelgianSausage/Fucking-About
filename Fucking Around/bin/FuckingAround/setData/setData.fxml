<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #ededeb;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FuckingAround.setData.setDataController">
   <children>
      <SplitPane dividerPositions="0.5" prefHeight="600.0" prefWidth="900.0" AnchorPane.leftAnchor="0.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <Text layoutX="145.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time activities">
                     <font>
                        <Font name="Calisto MT" size="25.0" />
                     </font>
                  </Text>
                  <TextField fx:id="nameField" layoutX="12.0" layoutY="85.0" prefHeight="49.0" prefWidth="423.0" promptText="                                 Please name the activity being completed" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" />
                  <Label fx:id="timerLabel" layoutX="30.0" layoutY="263.0" prefHeight="146.0" prefWidth="386.0" text="00:00:00.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
                     <font>
                        <Font name="Calisto MT" size="87.0" />
                     </font>
                  </Label>
                  <Button fx:id="startButton" layoutX="63.0" layoutY="427.0" mnemonicParsing="false" onAction="#startTimer" prefHeight="108.0" prefWidth="320.0" text="Start" AnchorPane.leftAnchor="63.0" AnchorPane.rightAnchor="63.0">
                     <font>
                        <Font size="34.0" />
                     </font>
                  </Button>
                  <Text layoutX="105.0" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Start time for activity:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Text>
                  <ComboBox fx:id="hourTimerDropdown" layoutX="63.0" layoutY="217.0" prefWidth="150.0" promptText="Hour" AnchorPane.leftAnchor="60.0" />
                  <ComboBox fx:id="minuteDropdown" layoutX="226.0" layoutY="217.0" prefWidth="150.0" promptText="Minute" AnchorPane.rightAnchor="60.0" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <Text layoutX="151.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manual input">
                     <font>
                        <Font name="Calisto MT" size="25.0" />
                     </font>
                  </Text>
                  <TextField fx:id="nameFieldManual" layoutX="12.0" layoutY="86.0" prefHeight="49.0" prefWidth="423.0" promptText="                                 Please name the activity being completed" />
                  <Text layoutX="106.0" layoutY="184.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Start time for activity:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Text>
                  <Text layoutX="110.0" layoutY="296.0" strokeType="OUTSIDE" strokeWidth="0.0" text="End time for activity:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Text>
                  <Button fx:id="resetButton" layoutX="12.0" layoutY="476.0" mnemonicParsing="false" onAction="#resetActivity" prefHeight="49.0" prefWidth="423.0" text="Reset" />
                  <Button fx:id="setActivityButton" layoutX="12.0" layoutY="374.0" mnemonicParsing="false" onAction="#setActivity" prefHeight="87.0" prefWidth="423.0" text="Set Activity" />
                  <Button fx:id="backButton" layoutX="12.0" layoutY="535.0" mnemonicParsing="false" onAction="#goToHomepage" prefHeight="49.0" prefWidth="423.0" text="Back" />
                  <ComboBox fx:id="hourStartManualDropdown" layoutX="63.0" layoutY="216.0" prefWidth="150.0" promptText="Hour" />
                  <ComboBox fx:id="minuteStartManualDropdown" layoutX="232.0" layoutY="216.0" prefWidth="150.0" promptText="Minute" />
                  <ComboBox fx:id="hourEndManualDropdown" layoutX="63.0" layoutY="326.0" prefWidth="150.0" promptText="Hour" />
                  <ComboBox fx:id="minuteEndManualDropdown" layoutX="232.0" layoutY="326.0" prefWidth="150.0" promptText="Minute" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
